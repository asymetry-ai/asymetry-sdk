# Datametry SDK Configuration

# Your Datametry API Key (get from https://datametry.co/dashboard)
DATAMETRY_API_KEY=datametry_dummy_key_123

# API endpoint (defaults to production)
DATAMETRY_API_URL=http://localhost:8080/v1/ingest

# Enable/disable observability
DATAMETRY_ENABLED=true

# Batch configuration
DATAMETRY_BATCH_SIZE=100
DATAMETRY_FLUSH_INTERVAL=5

# Queue configuration
DATAMETRY_QUEUE_MAX_SIZE=10000
DATAMETRY_MAX_RETRIES=3